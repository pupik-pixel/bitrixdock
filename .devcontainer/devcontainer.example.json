{
    "name": "bitrixdock",
    "dockerComposeFile": "../docker-compose.yml",
    "remoteUser": "www-data",
    "runServices": [
        "workspace"
    ],
    "service": "workspace",
    "workspaceFolder": "/var/www/bitrix",
    "shutdownAction": "stopCompose",
    "mounts": [
        "source=vscode-extensions,target=/var/www/.vscode-server,type=volume"
    ]
}